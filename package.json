{
  "name": "pw-js",
  "version": "1.0.0",
  "description": "Manages encrypted password files",
  "scripts": {
    "test": "echo 'No tests found'",
    "clean": "rimraf dist",
    "build": "npm run test && npm run clean && pkg src/pw.js --targets node8-linux-x64,node8-macos-x64,node8-win-x64 --out-path dist",
    "format-code": "find src -name '*.js' | xargs -I{} ./node_modules/.bin/prettier --write --single-quote {}"
  },
  "author": "yurezu",
  "repository": "https://github.com/gnosec/pw-js",
  "license": "GNU",
  "bin": {
    "pw": "src/pw.js"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "copy-paste": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "inquirer": "^5.1.0",
    "mkdirp": "^0.5.1",
    "npm": "^5.7.1",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "pkg": "^4.3.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  }
}
