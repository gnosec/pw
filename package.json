{
  "name": "pw",
  "version": "1.0.0",
  "description": "Intuitive cross-platform command line password manager",
  "license": "GPL-3.0-only",
  "author": {
    "name": "gnosec",
    "url": "https://github.com/gnosec"
  },
  "repository": "git@github.com/gnosec/pw",
  "bugs": {
    "url": "https://github.com/gnosec/pw/issues"
  },
  "scripts": {
    "test": "jasmine 'lib/**/*.spec.js'",
    "cover": "istanbul cover node_modules/.bin/jasmine 'lib/**/*.spec.js'",
    "clean": "rimraf bin",
    "build": "npm run test && npm run clean && pkg lib/pw.js --targets node8-linux-x64,node8-macos-x64,node8-win-x64 --out-path bin",
    "format-code": "find lib -name '*.js' | xargs -I{} ./node_modules/.bin/prettier --write --single-quote {}"
  },
  "bin": {
    "pw": "lib/pw.js"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "copy-paste": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "get-parameter-names": "^0.3.0",
    "inquirer": "^5.1.0",
    "mkdirp": "^0.5.1",
    "semver": "^5.5.0",
    "treeify": "^1.1.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "npm": "^5.8.0",
    "pkg": "^4.3.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  }
}
