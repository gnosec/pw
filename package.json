{
  "name": "pw",
  "version": "1.0.0",
  "description": "Intuitive cross-platform command line password manager",
  "license": "GNU General Public License v3.0",
  "author": {
    "name": "gnosec",
    "url": "https://github.com/gnosec"
  },
  "repository": "git@github.com/gnosec/pw",
  "bugs": {
    "url": "https://github.com/gnosec/pw/issues"
  },
  "scripts": {
    "test": "jasmine test/**/*.spec.js",
    "clean": "rimraf bin",
    "build": "npm run test && npm run clean && pkg lib/pw.js --targets node8-linux-x64,node8-macos-x64,node8-win-x64 --out-path bin",
    "format-code": "find lib -name '*.js' | xargs -I{} ./node_modules/.bin/prettier --write --single-quote {}"
  },
  "bin": {
    "pw": "lib/pw.js"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "copy-paste": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "inquirer": "^5.1.0",
    "mkdirp": "^0.5.1",
    "npm": "^5.7.1",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "jasmine": "^3.1.0",
    "pkg": "^4.3.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  }
}
